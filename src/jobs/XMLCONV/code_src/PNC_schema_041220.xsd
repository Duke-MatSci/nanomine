<xsd:schema xmlns:hdf5="http://hdfgroup.org/HDF5/XML/schema/HDF5-File"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    attributeFormDefault="unqualified">
    <xsd:element name="PolymerNanocomposite" type="Root">
        <xsd:annotation>
            <xsd:documentation>main polymer nanocomposite (PNC) sample entry </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="MatrixType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="MatrixComponent"
                type="PolymerType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="MatrixProcessing"
                type="ProcessingMethodType"/>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="RheometryType">
        <xsd:sequence>
            <xsd:element name="RheometerType" type="RheometerType"/>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="CapillarySize" type="PositiveValueUnitType"/>
            <xsd:element minOccurs="0" name="Equipment" type="xsd:string"/>
            <xsd:element minOccurs="0" name="data" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="RheometerType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="plate"/>
            <xsd:enumeration value="cone-plate"/>
            <xsd:enumeration value="high pressure capillary"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="ExtrusionType">
        <xsd:choice>
            <xsd:element name="SingleScrewExtrusion" type="SingleScrewExtrusionExtruderType"/>
            <xsd:element name="TwinScrewExtrusion" type="TwinScrewExtrusionExtruderType"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="ExtruderOutput">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="MeltTemperature" type="TemperatureType"/>
            <xsd:element minOccurs="0" name="PressureAtDie" type="PressureType"/>
            <xsd:element minOccurs="0" name="Torque" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Amperage" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Voltage" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Power" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="ThroughPut" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="ResidenceTime" type="TimeType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Single"/>
    <xsd:complexType name="GeneralConditionsType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Temperature" type="TemperatureType"/>
            <xsd:element minOccurs="0" name="Time" type="TimeType"/>
            <xsd:element minOccurs="0" name="Pressure" type="PressureType"/>
            <xsd:element minOccurs="0" name="AmbientCondition" type="ProcessingParameterAtmosphere"
            />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="FillerType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="FillerComponent"
                type="ParticleType"/>
            <xsd:element minOccurs="0" name="FillerProcessing" type="ProcessingMethodType"/>
            <xsd:element minOccurs="0" name="FillerComposition" type="PolymerCompositionTypeWithSource"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="RheologicalType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="RheologicalComplexModulus"
                type="RheologicalPropertiesType" maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="RheologicalViscosity" type="ViscosityType"
                maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="ElectricConductivityACDCType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="AC"/>
            <xsd:enumeration value="DC"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="IrregularShapeType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Width" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Length" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Depth" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Distribution">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="AxisLabel" type="AxisLabelType"/>
            <xsd:element xmlns:ns0="http://mdcs.ns" name="data"
                ns0:_mod_mdcs_="/diffusion/upload-excel"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ScalarUncertainty">
        <xsd:sequence>
            <xsd:element name="description" type="xsd:string" minOccurs="0"/>
            <xsd:element name="value" type="xsd:double" minOccurs="0"/>
            <xsd:element name="unit" type="xsd:string" minOccurs="0"/>
            <xsd:element minOccurs="0" name="uncertainty" type="Uncertainty"/>
            <xsd:element minOccurs="0" name="data" type="Distribution"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="UsedType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Used"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="ProcessingCategoryType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="ExperimentalProcedure"
                type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Descriptive text that includes all procedures in experimental synthesis and fabrication of final nanocomposites. </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="SolutionProcessing"
                type="ProcessingMethodType">
                <xsd:annotation>
                    <xsd:documentation>PNC processing type I: solution/solvent based processing</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="MeltMixing"
                type="ProcessingMethodType">
                <xsd:annotation>
                    <xsd:documentation>PNC processing type II: mixing and extrusion</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="In-SituPolymerization"
                type="ProcessingMethodType">
                <xsd:annotation>
                    <xsd:documentation>PNC processing type III: in-situ polymerization</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Other_Processing"
                type="ProcessingMethodType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ProcessingMethodType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="ChooseParameter"
                type="ProcessingParameterType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ProcessingParameterType">
        <xsd:choice>
            <xsd:element name="Additive" type="ProcessingParameterAdditive"/>
            <xsd:element minOccurs="1" name="Curing" type="GeneralConditionsType"/>
            <xsd:element name="Solvent" type="SolventType"/>
            <xsd:element name="Mixing" type="ProcessingParameterMixing"/>
            <xsd:element name="Extrusion" type="ExtrusionType"/>
            <xsd:element name="Heating" type="GeneralConditionsType"/>
            <xsd:element minOccurs="1" name="Cooling" type="GeneralConditionsType"/>
            <xsd:element name="Drying-Evaporation" type="GeneralConditionsType"/>
            <xsd:element name="Centrifugation" type="UsedType"/>
            <xsd:element name="Molding" type="MoldingDescriptionType"/>
            <xsd:element name="Other" type="xsd:string"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:simpleType name="ProcessingParameterMolding">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="hot-pressing"/>
            <xsd:enumeration value="casting"/>
            <xsd:enumeration value="injection molding"/>
            <xsd:enumeration value="rotational molding"/>
            <xsd:enumeration value="vacuum molding"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ProcessingParameterAtmosphere">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="atmosphere"/>
            <xsd:enumeration value="vacuum"/>
            <xsd:enumeration value="inert (nitrogen, argon, etc)"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="TimeTemperatureDescription"> </xsd:complexType>
    <xsd:complexType name="ProcessingParameterMixer">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="MixerUsed" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ProcessingParameterMixing">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
            <xsd:element name="Mixer" type="xsd:string" minOccurs="0"/>
            <xsd:element minOccurs="0" name="MixingMethod" type="ProcessingParameterMixingMethod"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="ChemicalUsed"
                type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="RPM" type="PositiveValueUnitType"/>
            <xsd:element minOccurs="0" name="Time" type="TimeType"/>
            <xsd:element minOccurs="0" name="Temperature" type="TemperatureType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="ProcessingParameterMixingMethod">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ball milling"/>
            <xsd:enumeration value="ultra-sonication"/>
            <xsd:enumeration value="stirring"/>
            <xsd:enumeration value="mechanical mixing"/>
            <xsd:enumeration value="high shear mixing"/>
            <xsd:enumeration value="roll milling"/>
            <xsd:enumeration value="dissolving"/>
            <xsd:enumeration value="blending"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="ProcessingParameterAdditive">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Additive" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Amount" type="PositiveValueUnitType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="VolumetricType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="WeightLoss"
                type="ScalarUncertainty"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="InterphaseThickness"
                type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Density" type="PositiveValueUnitType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="LinearExpansionCoefficient" type="ScalarUncertainty"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="VolumeExpansionCoefficient" type="ScalarUncertainty"
                maxOccurs="unbounded"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="SurfaceTension"
                type="ScalarUncertainty"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="InterfacialTension"
                type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="WaterAbsorption" type="ScalarUncertainty"
                maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="ThermalMeasurementMethodType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="DSC"/>
            <xsd:enumeration value="DTA"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CrystallizationType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="DegreeOfCrystallization" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="GrowthRateOfCrystal" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="GrowthRateParameterOfAvramiEquation"
                type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="NucleationParameterOfAvramiEquation"
                type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="HalflifeOfCrystallization" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DSCMeasurementType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Equipment" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="HeatingRate" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="CoolingRate" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="CycleInformation" type="xsd:string"/>
            <xsd:element minOccurs="0" name="data" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DielectricDispersionType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="DielectricDispersionDependence" type="DDDependenceType"/>
            <xsd:element minOccurs="0" name="Dielectric_Real_Permittivity" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Dielectric_Loss_Permittivity" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Dielectric_Loss_Tangent" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DDDependenceType">
        <xsd:choice>
            <xsd:element name="FrequencyDependence" type="DDFrequencyDependenceType"/>
            <xsd:element name="TemperatureDependence" type="DDTemperatureDependenceType"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="DDFrequencyDependenceType">
        <xsd:sequence>
            <xsd:element name="condition" type="DDFrequencyConditionType"
                minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DDFrequencyConditionType">
        <xsd:sequence minOccurs="1">
            <xsd:element name="temperature" type="TemperatureType" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DDTemperatureDependenceType">
        <xsd:sequence>
            <xsd:element name="condition" type="DDTemperatureConditionType"
                minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DDTemperatureConditionType">
        <xsd:sequence minOccurs="1">
            <xsd:element name="frequency" type="ScalarUncertainty" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CreepType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Compressive" type="CompressiveCreepType"/>
            <xsd:element minOccurs="0" name="Tensile" type="TensileCreepType"/>
            <xsd:element minOccurs="0" name="Flexural" type="FlexuralCreepType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="TensileCreepType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="TensileCreepRecovery" type="xsd:string"/>
            <xsd:element minOccurs="0" name="TensileCreepModulus" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="TensileCreepCompliance" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="TensileCreepRuptureStrength" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="TensileCreepRuptureTime" type="TimeType"/>
            <xsd:element minOccurs="0" name="TensileCreepStrain" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="FlexuralCreepType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="FlexuralCreepRuptureStrength" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="FlexuralCreepRuptureTime" type="TimeType"/>
            <xsd:element minOccurs="0" name="FlexuralCreepStrain" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CompressiveCreepType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CompressiveCreepRuptureStrength"
                type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="CompressiveCreepRuptureTime" type="TimeType"/>
            <xsd:element minOccurs="0" name="CompressiveCreepStrain" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="ViscoelasticMethodType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="tensile tester"/>
            <xsd:enumeration value="DMA"/>
            <xsd:enumeration value="ultrasonic"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="PolymerCompositionType">
        <xsd:sequence>
            <xsd:element name="Constituent" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Name of constituent substance</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Fraction" type="Percentage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Mass or volume fraction</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PolymerCompositionTypeWithSource">
        <xsd:sequence>
            <xsd:element name="Constituent" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Name of constituent substance</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Fraction" type="PercentageWithSource" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Mass or volume fraction</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="PolymerTypeType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="homopolymer"/>
            <xsd:enumeration value="copolymer"/>
            <xsd:enumeration value="polymer blend"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="MaterialClassType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="thermoplastic"/>
            <xsd:enumeration value="thermoset"/>
            <xsd:enumeration value="elastomer"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="newSimpleType">
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
    <xsd:complexType name="Root">
        <xsd:sequence>
            <xsd:element name="ID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Unique identification of material sample/data point</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Control_ID" type="xsd:string" minOccurs="0"/>
            <xsd:element name="SchemaVersion" type="xsd:string" maxOccurs="1"/>
            <xsd:element name="SchemaID" type="xsd:string" maxOccurs="1"/>
            <xsd:element name="DatasetID" type="xsd:string" maxOccurs="1"/>
            <xsd:element minOccurs="0" name="DATA_SOURCE" type="DataSourceType">
                <xsd:annotation>
                    <xsd:documentation>Metadata about data source, publication info. </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="MATERIALS" nillable="true" type="MaterialsType">
                <xsd:annotation>
                    <xsd:documentation>Material constituent charateristics </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PROCESSING" type="ProcessingCategoryType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Experimental processing conditions</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CHARACTERIZATION" type="MeasurementMethodsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Experimental characterization of material sample properties. Records info on instrument and equipment used</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PROPERTIES" type="PropertiesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Measured and reported sample properties</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MICROSTRUCTURE" minOccurs="0" type="MicrostructureDescriptorsType">
                <xsd:annotation>
                    <xsd:documentation>Microstructure of nanocomposite dispersion </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="GeneralEquipmentType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Equipment" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="data" type="ScalarUncertainty" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="TacticityType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="isotactic"/>
            <xsd:enumeration value="syndiotactic"/>
            <xsd:enumeration value="atactic"/>
            <xsd:enumeration value="eutactic"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="CitationFieldType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="research article"/>
            <xsd:enumeration value="conference proceeding"/>
            <xsd:enumeration value="communication"/>
            <xsd:enumeration value="review"/>
            <xsd:enumeration value="letter"/>
            <xsd:enumeration value="technical comment"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="SurfaceTensionType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="dispersive" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="polar" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ProcessingGeneralType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="TotalEnergyInput" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Throughput" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="MaxShearStress" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SurfaceModificationTypes">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="SurfaceTreatment"
                type="ParticleSurfaceModificationType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DataSourceType">
        <xsd:sequence>
            <xsd:element name="Citation" type="CitationType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Published data source</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LabGenerated" type="LabGeneratedType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Data from lab reports that are unpublished</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="LabGeneratedType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Experimenter" type="xsd:string"/>
            <xsd:element minOccurs="0" name="InstitutionOrLab" type="xsd:string"/>
            <xsd:element name="DateOfSampleMade" type="xsd:string" minOccurs="0"/>
            <xsd:element minOccurs="0" name="DateOfMeasurement" type="xsd:string"/>
            <xsd:element minOccurs="0" name="relatedDOI" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CitationBookType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Chapter" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Chapter number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Edition" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Edition number of the book</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Editor" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Editor of the book</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ISBN" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>The International Standard Book Number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CitationJournalType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ISSN" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>The International Standard Subscription Number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Issue" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Issue number, usually indicates month</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CitationDetailType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CitationType" type="CitationFieldType">
                <xsd:annotation>
                    <xsd:documentation>Type of publication
</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Publication" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Name of journal, conference, book, etc</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Title" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Title of the publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Author" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Author(s) of publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Keyword" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Keyword and index of the publication
</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Publisher" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Name of the publisher</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PublicationYear" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Publication year</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DOI" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>DOI</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Volume" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Volume number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="URL" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>URL to the publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Language" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Language of publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Location" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Location of conference, meeting, etc, of the publication
</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DateOfCitation" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Date of data entry</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CitationTypeType">
        <xsd:choice>
            <xsd:element name="Book" type="CitationBookType"/>
            <xsd:element name="Journal" type="CitationJournalType"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="CitationType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CommonFields" type="CitationDetailType">
                <xsd:annotation>
                    <xsd:documentation>Common attributes for a material sample from published data source</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CitationType" type="CitationTypeType">
                <xsd:annotation>
                    <xsd:documentation>Attributes specific to book chapter or journal publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="MaterialsType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Matrix" type="MatrixType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Polymer matrix </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Filler" type="FillerType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Nanoparticle filler</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PolymerType">
        <xsd:sequence>
            <xsd:element name="ChemicalName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Full name of polymer</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="StdChemicalName" type="xsd:string"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="SMILES" type="xsd:string"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="uSMILES" type="xsd:string"/>
            <xsd:element minOccurs="0" name="PubChemRef" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Abbreviation" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Chemical structure, short name, or abbreviation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ConstitutionalUnit" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Constitutional unit (CU) formula of homopolymer or a component </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PlasticType" type="MaterialClassType">
                <xsd:annotation>
                    <xsd:documentation>Type of plastics based on curing and cross-linking
</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PolymerClass" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Type of polymer based on CU and CAS structure</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PolymerType" type="PolymerTypeType">
                <xsd:annotation>
                    <xsd:documentation>Type of polymer based on monomer characteristics </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ManufacturerOrSourceName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="TradeName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Brand, commercial name of polymer matrix</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="MolecularWeight" type="PositiveValueUnitType"
                maxOccurs="unbounded"> </xsd:element>
            <xsd:element minOccurs="0" name="Polydispersity" type="PolydispersityType"/>
            <xsd:element minOccurs="0" name="Tacticity" type="TacticityType" maxOccurs="1"/>
            <xsd:element minOccurs="0" name="Density" type="PositiveValueUnitType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Viscosity" type="PositiveValueUnitType"
                maxOccurs="unbounded"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Hardener" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>For epoxy resins</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Additive"
                type="ScalarUncertainty">
                <xsd:annotation>
                    <xsd:documentation>Any additional additive, e.g., catalyst, promoter </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="MatrixComponentComposition"
                type="PolymerCompositionType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>For matrix consisting of more than one type of polymer materials </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ParticleType">
        <xsd:sequence>
            <xsd:element name="ChemicalName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Full name of filler particle substance</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="StdChemicalName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="PubChemRef" type="xsd:string"/>
            <xsd:element name="Abbreviation" type="xsd:string" minOccurs="0"/>
            <xsd:element minOccurs="0" name="ManufacturerOrSourceName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="TradeName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Density" type="PositiveValueUnitType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="CrystalPhase" type="xsd:string"/>
            <xsd:element minOccurs="0" name="SphericalParticleDiameter" maxOccurs="unbounded"
                type="ScalarStdUncertainty"/>
            <xsd:element minOccurs="0" name="SurfaceArea" type="SurfaceAreaType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="ParticleAspectRatio" type="ScalarUncertainty"
                maxOccurs="unbounded"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="NonSphericalShape"
                type="IrregularShapeType"/>
            <xsd:element minOccurs="0" name="FillerComponentComposition" type="Percentage"/>
            <xsd:element minOccurs="0" name="FillerComponentInComposite" type="Percentage"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParticleSurfaceTreatment"
                type="ParticleSurfaceModificationType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ParticleSurfaceModificationType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ChemicalName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Abbreviation" type="xsd:string"/>
            <xsd:element name="ConstitutionalUnit" nillable="false" type="xsd:string" minOccurs="0"/>
            <xsd:element minOccurs="0" name="ManufacturerOrSourceName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="TradeName" type="xsd:string"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Density"
                type="PositiveValueUnitType"/>
            <xsd:element minOccurs="0" name="GraftDensity" type="PositiveValueUnitType"
                maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>For grafted chains </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="MolecularWeight" type="PositiveValueUnitType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="PST_Composition" type="PolymerCompositionType"
                maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>For multiple populations grafted chains</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SurfaceChemistryProcessing" type="ProcessingMethodType"
            />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="AspectRatioType">
        <xsd:sequence>
            <xsd:element name="mean" type="xsd:string"/>
            <xsd:element name="variance" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ProcessingType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="PolymerType" type="MethodType"/>
            <xsd:element minOccurs="0" name="GeneralParameters" type="ProcessingGeneralType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="MethodType">
        <xsd:choice>
            <xsd:element name="Thermoplastic" type="ThermoplasticProcessingType"/>
            <xsd:element name="Thermoset" type="ThermosetProcessingType"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="MeasurementMethodsType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Transmission_Electron_Microscopy" type="SEMTEMtype"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Scanning_Electron_Microscopy" type="SEMTEMtype"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Dynamic_Mechanical_Analysis"
                type="GeneralEquipmentType" maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Dielectric_and_Impedance_Spectroscopy_Analysis"
                type="GeneralEquipmentType" maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Differential_Scanning_Calorimetry"
                type="DSCMeasurementType" maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Fourier_Transform_Infrared_Spectroscopy"
                type="GeneralEquipmentType" maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="XRay_Diffraction_and_Scattering"
                type="GeneralEquipmentType" maxOccurs="unbounded"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="XRay_Photoelectron_Spectroscopy"
                type="GeneralEquipmentType"/>
            <xsd:element minOccurs="0" name="Atomic_Force_Microscopy" type="GeneralEquipmentType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Thermogravimetric_Analysis" type="GeneralEquipmentType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Raman_Spectroscopy" type="GeneralEquipmentType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Nuclear_Magnetic_Resonance" type="GeneralEquipmentType"
                maxOccurs="unbounded"/>
            <xsd:element minOccurs="0" name="Pulsed_Electro_Acoustic" type="GeneralEquipmentType"
                maxOccurs="unbounded"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Rheometery" type="RheometryType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Electrometry"
                type="GeneralEquipmentType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="OpticalMicroscopy"
                type="GeneralEquipmentType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PropertiesType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Mechanical" type="MechanicalType"/>
            <xsd:element minOccurs="0" name="Viscoelastic" type="ViscoelasticType"/>
            <xsd:element minOccurs="0" name="Electrical" type="DielectricPropertiesType"/>
            <xsd:element minOccurs="0" name="Thermal" type="ThermalType"/>
            <xsd:element minOccurs="0" name="Volumetric" type="VolumetricType"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="Rheological" type="RheologicalType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="BinaryType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Yes"/>
            <xsd:enumeration value="No"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="ThermoplasticProcessingType">
        <xsd:sequence>
            <xsd:element default="(usually the Experimental section)"
                name="Experimental_Procedure_description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="Solvent" type="xsd:string"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="PreProcessing"
                type="ThermoplasticPolymerType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CompositeProcessing"
                type="ThermoplasticMethodType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Polymerization"
                type="PolymerizationType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Drying" type="DryingType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="HotPressing"
                type="ThermosetProcessingPostMixingConditionsType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="OtherProcessingStep"
                type="ThermosetProcessingPostMixingConditionsType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ThermoplasticPolymerType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="granularSize" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="startingAgglomerateSize" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="viscosityAtMeltingTemperature" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="drying" type="ThermoplasticDryingType"/>
            <xsd:element default="" minOccurs="0" name="Pre_blending_method" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ThermoplasticMethodType">
        <xsd:choice>
            <xsd:element name="singleScrewExtrusion" type="SingleScrewExtrusionType"/>
            <xsd:element name="twinScrewExtrustion" type="TwinScrewExtrusionType"/>
            <xsd:element name="batchMixer" type="BatchMixerType"/>
            <xsd:element name="solutionProcessing" type="SolutionProcessingType"/>
            <xsd:element name="other_processing_method" type="xsd:string"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="ThermoplasticDryingType">
        <xsd:sequence>
            <xsd:element name="time" type="ScalarUncertainty"/>
            <xsd:element name="temperature" type="ScalarUncertainty"/>
            <xsd:element name="drying" type="UsedType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SingleScrewExtrusionType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ExtruderAndScrew"
                type="SingleScrewExtrusionExtruderType"/>
            <xsd:element minOccurs="0" name="parameters" type="SingleScrewExtrusionParametersType"/>
            <xsd:element minOccurs="0" name="output" type="SingleScrewExtrusionOutputType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="TwinScrewExtrusionType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ExtruderAndScrew" type="TwinScrewExtrusionExtruderType"/>
            <xsd:element minOccurs="0" name="parameters" type="TwinScrewExtrusionParametersType"/>
            <xsd:element minOccurs="0" name="output" type="SingleScrewExtrusionOutputType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="BatchMixerType">
        <xsd:sequence>
            <xsd:element name="temperature" type="ScalarUncertainty"/>
            <xsd:element name="rotationSpeed" type="ScalarUncertainty"/>
            <xsd:element name="time" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SolutionProcessingType">
        <xsd:sequence>
            <xsd:element name="solvent" type="xsd:string"/>
            <xsd:element name="mixingDevice" type="MixingDeviceType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="MixingDeviceType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ultrasonic"/>
            <xsd:enumeration value="dissolver"/>
            <xsd:enumeration value="hand stir"/>
            <xsd:enumeration value="stir"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="TwinScrewExtrusionExtruderType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Extruder" type="xsd:string"/>
            <xsd:element minOccurs="0" name="ResidenceTime" type="TimeType"/>
            <xsd:element minOccurs="0" name="ExtrusionTemperature" type="TemperatureType"/>
            <xsd:element minOccurs="0" name="RotationMode" type="RotationType"/>
            <xsd:element minOccurs="0" name="RotationSpeed" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="ScrewDiameter" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="ScrewChannelDiameter" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="D_L_ratio" type="PositiveValueType"/>
            <xsd:element minOccurs="0" name="FlightClearance" type="ScalarUncertainty"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="HeatingZone"
                type="HeatingZoneType"/>
            <xsd:element minOccurs="0" name="DieDiameter" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="Output" type="ExtruderOutput"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="TwinScrewExtrusionParametersType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="dieTemperature" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="rotationSpeed" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="locationOfFeeding" type="LocationOfFeedingType"/>
            <xsd:element minOccurs="0" name="FeedingRate" type="FeedingRateType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SingleScrewExtrusionOutputType">
        <xsd:sequence>
            <xsd:element name="MeltTemperature" type="ScalarUncertainty"/>
            <xsd:element name="PressureAtDie" type="ScalarUncertainty"/>
            <xsd:element name="torque" type="ScalarUncertainty"/>
            <xsd:element name="amperage" type="ScalarUncertainty"/>
            <xsd:element name="voltage" type="ScalarUncertainty"/>
            <xsd:element name="throughput" type="ScalarUncertainty"/>
            <xsd:element name="residenceTime" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="LocationOfFeedingType">
        <xsd:sequence>
            <xsd:element name="polymer" type="ScalarUncertainty"/>
            <xsd:element name="nanoparticles" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="FeedingRateType">
        <xsd:sequence>
            <xsd:element name="polymer" type="ScalarUncertainty"/>
            <xsd:element name="nanoparticles" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="RotationType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="CoRotation"/>
            <xsd:enumeration value="CounterRotation"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="HeatingZoneType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="heatingZoneNumber" type="xsd:nonNegativeInteger"/>
            <xsd:element minOccurs="0" name="lengthOfHeatingZone" type="ScalarUncertainty"/>
            <xsd:element minOccurs="0" name="barrelTemperature" type="TemperatureType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SingleScrewExtrusionExtruderType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Extruder" type="xsd:string"/>
            <xsd:element minOccurs="0" name="ResidenceTime" type="TimeType"/>
            <xsd:element minOccurs="0" name="ExtrusionTemperature" type="TemperatureType"/>
            <xsd:element name="ScrewDiameter" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element name="InnerBarrelDiameter" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element name="ScrewLength" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element name="D_L_ratio" type="PositiveValueType" minOccurs="0"/>
            <xsd:element name="RadialFlightClearance" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element name="FlightWidth" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element name="ChannelDepth" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element name="ScrewLead" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element name="NumberOfChannelsPerScrew" type="xsd:nonNegativeInteger" minOccurs="0"/>
            <xsd:element name="ScrewChannelWidth" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="HeatingZone"
                type="HeatingZoneType"/>
            <xsd:element name="DieDiameter" type="ScalarUncertainty" minOccurs="0"/>
            <xsd:element minOccurs="0" name="RotationSpeed" type="ScalarUncertainty"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="BarrelTemperature"
                type="TemperatureType"/>
            <xsd:element minOccurs="0" name="Output" type="ExtruderOutput"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SingleScrewExtrusionParametersType">
        <xsd:sequence>
            <xsd:element name="rotationSpeed" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PolymerizationType">
        <xsd:sequence>
            <xsd:element name="Heating" type="UsedType"/>
            <xsd:element name="description" type="xsd:string"/>
            <xsd:element name="time" type="ScalarUncertainty"/>
            <xsd:element name="temperature" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DryingType">
        <xsd:sequence>
            <xsd:element name="vacuum_oven" type="UsedType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Drying_step"
                type="ThermosetProcessingPostMixingConditionsType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ThermosetProcessingPostMixingConditionsType">
        <xsd:sequence>
            <xsd:element name="description" type="xsd:string"/>
            <xsd:element name="time" type="ScalarUncertainty"/>
            <xsd:element name="temperature" type="ScalarUncertainty"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ThermosetProcessingType">
        <xsd:sequence>
            <xsd:element default="(usually the Experimental section)"
                name="Experimental_Procedure_description" type="xsd:string"/>
            <xsd:element minOccurs="0" name="EpoxyType" type="ThermosetProcessingChemistryType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Solvent" type="xsd:string"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="PreProcessing"
                type="ThermosetProcessingPreTreatmentType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CompositeProcessing"
                type="ThermosetProcessingMethodType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Polymerization"
                type="PolymerizationType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Drying" type="DryingType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="HotPressing"
                type="ThermosetProcessingPostMixingConditionsType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="OtherProcessingStep"
                type="ThermosetProcessingPostMixingConditionsType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="ThermosetProcessingChemistryType">
