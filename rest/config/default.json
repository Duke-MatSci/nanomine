    "logfilename": "nanomine.log",
    "maxlogfiles": 10,
    "maxlogfilesize": 1024000,
    "testmode": {
      "active": false
    },
    "rest": {
      "dataSizeLimit": "10mb"
    },
    "jobtypes": [
      {
        "jobtype": "example",
        "lang": "python",
        "program": "../src/jobs/example/run_example.py"
      },
      {
        "jobtype": "xmlconv",
        "lang": "python",
        "program": "../src/jobs/XMLCONV/run_xmlconv.py"
      }
    ]
  }
}
