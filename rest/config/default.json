{
  "nanomine": {
    "version": "0.1.0",
    "loglevel": "debug",
    "logfilename": "nanomine.log",
    "maxlogfiles": 10,
    "maxlogfilesize": 1024000,
    "testmode": {
      "active": false
    },
    "rest": {
      "dataSizeLimit": "10mb"
    },
    "jobtypes": [
      {
        "jobtype": "example",
        "lang": "python",
        "pgmdir": "../src/jobs/example",
        "pgmname": "run_example.py"
      },
      {
        "jobtype": "otsu",
        "lang": "python",
        "pgmdir": "../src/jobs/otsu",
        "pgmname": "run_otsu.py"
      },
      {
        "jobtype": "xmlconv",
        "lang": "python",
        "pgmdir": "../src/jobs/XMLCONV",
        "pgmname": "run_xmlconv.py"
      },
      {
        "jobtype": "SDFCharacterize",
        "lang": "python",
        "pgmdir": "../src/jobs/SDF",
        "pgmname": "run_SDFCharacterize.py"
      },
      {
        "jobtype": "DescriptorCharacterize",
        "lang": "python",
        "pgmdir": "../src/jobs/Descriptor",
        "pgmname": "run_DescriptorCharacterize.py"
      },
      {
        "jobtype": "CorrelationCharacterize",
        "lang": "python",
        "pgmdir": "../src/jobs/CorrelationFunctions",
        "pgmname": "run_CorrelationCharacterize.py"
      },
      {
        "jobtype": "SDFReconstruct",
        "lang": "python",
        "pgmdir": "../src/jobs/SDF",
        "pgmname": "run_SDFReconstruct.py"
      },
      {
        "jobtype": "CorrelationReconstruct",
        "lang": "python",
        "pgmdir": "../src/jobs/CorrelationFunctions",
        "pgmname": "run_CorrelationReconstruct.py"
      }
    ]
  }
}
